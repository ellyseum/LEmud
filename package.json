    "ws": "^8.13.0",
    "uuid": "^9.0.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.4",
    "@types/uuid": "^9.0.1",
    "@types/socket.io": "^3.0.2",
    "typescript": "^4.9.4",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.22"
  }
}
